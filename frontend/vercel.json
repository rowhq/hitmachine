{
  "functions": {
    "app/api/generate-account/route.ts": {
      "maxDuration": 10
    },
    "app/api/purchase-giftcard/route.ts": {
      "maxDuration": 10
    },
    "app/api/complete-flow/route.ts": {
      "maxDuration": 10
    },
    "app/api/analytics/route.ts": {
      "maxDuration": 10
    },
    "app/api/analytics-dashboard/route.ts": {
      "maxDuration": 10
    },
    "app/api/cron/clawback/route.ts": {
      "maxDuration": 30
    },
    "app/api/cron/fund-management/route.ts": {
      "maxDuration": 10
    }
  },
  "crons": [
    {
      "path": "/api/cron/clawback",
      "schedule": "* * * * *"
    },
    {
      "path": "/api/cron/fund-management",
      "schedule": "* * * * *"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}