# Copy this file to .env.local and fill in your values

# Blockchain Configuration
WALLET_PRIVATE_KEY=your_deployer_wallet_private_key_here
MNEMONIC=your_twelve_word_mnemonic_phrase_here
RPC_URL=https://rpc.sophon.xyz
ETHERSCAN_SOPHON_API_KEY=your_etherscan_api_key_here

# Vercel KV Configuration
KV_REST_API_URL=your_vercel_kv_rest_api_url_here
