# Copy this file to .env.local and fill in your values

# Blockchain Configuration - Foundry
PRIVATE_KEY=your_deployer_wallet_private_key_here_without_0x_prefix
SOPHON_TESTNET_RPC_URL=https://rpc.testnet.sophon.xyz
SOPHON_MAINNET_RPC_URL=https://rpc.sophon.xyz
ETHERSCAN_SOPHON_API_KEY=your_etherscan_api_key_here

# Contract Addresses (set after deployment)
STORE_CONTRACT=
JOBS_CONTRACT=

# Optional: Additional role addresses
OPERATOR_ADDRESS=
DISTRIBUTOR_ADDRESS=

# API Configuration (for Vercel functions)
WALLET_PRIVATE_KEY=your_deployer_wallet_private_key_here
MNEMONIC=your_twelve_word_mnemonic_phrase_here
RPC_URL=https://rpc.sophon.xyz

# Mass Buy API Key (generate with: node scripts/generate-api-key.js)
MASS_BUY_API_KEY=

# Vercel KV Configuration
KV_REST_API_URL=your_vercel_kv_rest_api_url_here
KV_REST_API_TOKEN=your_vercel_kv_rest_api_token_here

# Supabase Configuration (for IP tracking)
SUPABASE_URL=https://your-project-ref.supabase.co
SUPABASE_ANON_KEY=your_supabase_anon_key_here  
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here
SUPABASE_POOLER_URL=https://your-project-ref.pooler.supabase.com

# Analytics API Key (optional - for protected analytics endpoint)
ANALYTICS_API_KEY=generate_random_string_here